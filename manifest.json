{
  "name": "rfd affiliate link removal",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Turns the rfd affiliate links to non-affiliate links.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://forums.redflagdeals.com/*"],
      "js": ["jquery-1.9.1.min.js","convert.js"],
      "run_at": "document_end"
    }
  ]
}
